<%= form_for @resource do |f| %>
  <%= f.text_field :email %>
  <%= f.password_field :password %>
  <%= f.select :organization_ids, Organization.pluck(:name, :id), {},
              data: { placeholder: "Select organizations", behavior: "select-box" },
              multiple: true %>

  <%= f.submit "Save", class: "btn btn-default" %>
<% end %>
